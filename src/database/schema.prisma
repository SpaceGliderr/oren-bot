datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
    id Int @default(autoincrement()) @id
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
    username String
    discriminator String
    defaultAvatarURL String?
}